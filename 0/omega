FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.78;

boundaryField
{
    

    inlet
	{
    	type  			fixedValue;
    	value 			$internalField;
	}

    outlet
    {
        type            fixedValue;
    	value 			$internalField;
    }

    road
    {
        type           	zeroGradient;
    }

    car
    {
        type            zeroGradient;
    }

    top
	{
    	type 				zeroGradient;
	}

	frontAndBack
	{
    	type				empty;
	}	
}


